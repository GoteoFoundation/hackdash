<div class="panel-heading" role="tab">
  <h4 class="panel-title">
    <a role="button" data-toggle="collapse" href="#prj-{{ project._id }}">
    {{project.title}}
    -
    {{project.leader.name}}
    </a></h4>
</div>

<div id="prj-{{ project._id }}" class="panel-collapse collapse" role="tabpanel">
  <dl>
  {{#each responses}}
    <dt><i class="fa fa-{{../fa question.type}} fa-fw"></i> {{ question.title }}</dt>
    <dd>{{ ../format this }}</dd>
  {{/each}}
  </dl>
</div>
